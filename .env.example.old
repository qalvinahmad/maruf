# Copy this file to .env.local and fill in your actual Supabase credentials

# Supabase Configuration
# Get these from: https://supabase.com/dashboard -> Your Project -> Settings -> API

NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Development Settings

# Hugging Face API Key for voice recognition
# Get your key from: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_api_key_here
NODE_ENV=development
SUPPRESS_REDIS_WARNINGS=true
ENABLE_REDIS_DEV=false

# Optional: Hugging Face Token for AI features
# Get this from: https://huggingface.co/settings/tokens
HF_TOKEN=your_hugging_face_token_here

SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here
